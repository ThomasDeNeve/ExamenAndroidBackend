@using devops_project_web_t4.Pages.Location
@page "/coworking/locatie"

<LocationSelector TitlePinkUnderline="coworking"
                  Message="@Message"
                  NavigateHIER="@NavigateToCoworkingOverzichtHIER" 
                  NavigateKluizen="@NavigateToCoworkingOverzichtKluizen"/>


@code {

    public void NavigateToCoworkingOverzichtHIER()
    {
        _navigationManager.NavigateTo("/coworking/hier/overzicht");
    }

    public void NavigateToCoworkingOverzichtKluizen()
    {
        _navigationManager.NavigateTo("/coworking/kluizen/overzicht");
    }
}
