@code
{
    [Inject]
    private NavigationManager UriHelper { get; set; }
    [Inject]
    private IReservationController Controller { get; set; }

    private List<int> _seatsReserved;

    private bool IsTaken { get; set; }

    [Parameter]
    public List<int> SeatsReserved
    {
        get { return _seatsReserved; }
        set
        {
            if (value == null)
            {
                _seatsReserved = Controller.GetSeatIdsReservedForDate(DateTime.Now);
            }
            else
            {
                _seatsReserved = value;
            }

            StateHasChanged();
        }
    }


    public void NavigateTo(int id)
    {
        UriHelper.NavigateTo("/coworking/reserveer/seatReservationForm/" + id, false);
    }
}